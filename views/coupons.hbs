    <script>
       function couponDelete (idtodelete){
              swal({
                    title: "Are you sure?",
                    text: "Delete this coupon!",
                    icon: "warning",
                    buttons: true,
                    dangerMode: true,
                    }).then((willDelete) => {
                       if (willDelete) {
                           axios.delete('/admin/coupon/'+idtodelete)
                             .then((data)=>{
                                 if(data.data.admin == 'noSession'){
                                        location.href = '/admin'
                                    }else if(data.data.deletesuccess){
                                        swal(
                                        'success!',
                                        'Coupon has been deleted',
                                        'success'
                                        ).then((reponse)=>{
                                          location.href = '/admin/coupons'
                                         })
                                  }else if(data.data.deleteerror){
                                    swal(
                                      'Oops!',
                                      'Somethins gone wrong !',
                                      'error'
                                   )                                
                                  }else if(data.data.catcherror){
                                    swal(
                                      'Oops!',
                                      'Somethins gone wrong !',
                                      'error'
                                    )                                
                                  }   
                                })
                               }
                             })
                           }
       
       function couponblock (idtoblock){
              swal({
                        title: "Are you sure?",
                        text: "Block this coupon!",
                        icon: "warning",
                        buttons: true,
                        dangerMode: true,
                    }).then((willDelete) => {
                        if (willDelete) {
                           axios.patch('/admin/coupon/'+idtoblock)
                             .then((data)=>{
                                 if(data.data.admin == 'noSession'){
                                     location.href = '/admin'
                                    }else if(data.data.blocksuccess){
                                    swal(
                                      "Poof! Coupon has been blocked!", {
                                       icon: "success",
                                    }).then((reponse)=>{
                                      location.href = '/admin/coupons'
                                      })
                                  }else if(data.data.blockerror){
                                    swal(
                                     "Poof! Somthing gone wrong!", {
                                      icon: "error",
                                     })                                
                                  }else if(data.data.catcherror){
                                    swal(
                                     "Poof! Somthing gone wrong!", {
                                      icon: "error",
                                    })                                
                                  }   
                                })
                        }
                     })
                 }
        
        function couponunblock (idtounblock){
              swal({
                        title: "Are you sure?",
                        text: "!You want unblock this coupon",
                        icon: "warning",
                        buttons: true,
                        dangerMode: true,
                    }).then((willDelete) => {
                        if (willDelete) {
                           axios.patch('/admin/coupon/'+idtounblock)
                             .then((data)=>{
                                 if(data.data.admin == 'noSession'){
                                      location.href = '/admin'
                                    }else if(data.data.unblocksuccess){
                                        swal(
                                        'success!',
                                        'Coupon has been Unblocked',
                                        'success'
                                   ).then((reponse)=>{
                                     location.href = '/admin/coupons'
