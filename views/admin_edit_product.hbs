                            {{/each}}
                          </select>
                        </div>
                        <div class="col-md-6 mb-3">
                          <label for="">Quantity</label>
                          <input
                            type="number"
                            class="form-control"
                            id="productquantity"
                            name="productquantity"
                            value="{{foundproduct.quantity}}"
                          />
                        </div>
                       
                      </div>
                    </div>
                  </div>
                
