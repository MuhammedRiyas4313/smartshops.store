    <script>
      function testswal(){
         
        event.preventDefault()
        Swal.fire({
              title: 'Order has been canceled',
              target: '#custom-target',
              icon: 'success',
            })
             
        console.log('function is calling for sweet alert')
      }

      function cancelOrder(orderId){
        console.log('function is calling for cancel the order')
        event.preventDefault();
        Swal.fire({
          title: 'Are you sure?',
          target: document.getElementById('custom-target'),
          text: "You won't be able to revert this!",
          iconHtml: '?',
          showCancelButton: true,
          confirmButtonColor: '#3085d6',
          cancelButtonColor: '#d33',
          confirmButtonText: 'Yes, cancel it!'
        }).then((result) => {
          if (result.isConfirmed) {
            axios.patch('/order-cancel?id='+orderId).then((res)=>{
              console.log(res.data.message,'message from the axios res')
              if(res.data.message == 'Order canceled')
               Swal.fire({
                  title: 'Order has been canceled',
                  target: '#custom-target',
                  icon: 'success',
                }).then((re)=>{
                  document.getElementById('orderstatus').innerHTML = `<span class="font-weight-700">Status:</span> Canceled` 
                  document.getElementById('expectingdate').innerHTML = `<span class="font-weight-700">Expected Date:</span> _ / _ / _`
                  document.getElementById('cancelation').innerHTML = `<div class="step" id="confirmed">
                                                                        <i class="fa-regular fa-circle-xmark text-danger"></i>
                                                                          <h2 class="mb-0">Order canceled</h2>
                                                                      </div>`
                  document.getElementById('cancelbutton').innerHTML = ''

                })
            })
          }
        })
      }
    </script>
