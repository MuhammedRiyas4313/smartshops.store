                                        </div>
                                      {{/equal}}
                                     {{#equal trackOrder.status "Shipped"}}
                                        <div class="step" id="confirmed">
                                            <i class="fa-solid fa-circle-check text-success"></i>
                                            <h4 class="mb-0">Confirmed</h4>
                                        </div> 
                                       
                                        <div class="step" id="processing">
                                            <i class="fa-solid fa-circle-check text-success"></i>
                                            <h4 class="mb-0">Processing</h4>
                                        </div>
                                   
                                        <div class="step" id="shipped">
                                          <i class="fa-solid fa-circle-check text-success"></i>
                                          <h4 class="mb-0">Shipped</h4> 
                                        </div>
                                       
                                        <div class="step" id="delivered">
