    <script>
       const form = document.getElementById('addtocart');
        form.addEventListener('submit',(e)=>{
          console.log(' function is calling for the addtocart')
          const productname = document.getElementById(`wishlistproduct`).innerHTML
          e.preventDefault();
          const formData = new FormData(form)
          const data = Object.fromEntries(formData)
          const productId = data._id
          console.log(productId,'data from the form add to cart')
          axios.post('/cart/'+productId,data).then((data)=>{
            console.log(data.data,'data from the response')
            if(data.data.user == 'noSession' ){
                Swal.fire({
                  title: 'Do you want add to Cart ?',
                  text: "login please...",
                  icon: 'question',
                  showCancelButton: true,
                  confirmButtonColor: '#3085d6',
                  cancelButtonColor: '#d33',
                  confirmButtonText: 'Yes, login !'
                }).then((result) => {
                  if (result.isConfirmed) {
                    location.href = '/user-login'
                  }
                })
            }else if(data.data.message == 'Added to cart'){
              Swal.fire({
                            icon: 'success',
                            title: productname,
                            text: 'Added to cart !'
                          }).then((dat)=>{
                            location.href = '/cart'
                          })
            }else{
              Swal.fire({
                            icon: 'error',
                            title: 'Oops',
                            text: data.data.message
                          })           
             }
          })
        })
    </script>
