					}).then((result) => {
					if (result.isConfirmed) {
						location.href = '/user-login'
					}
					})
                }else if( response.data.applied ){
					document.getElementById('couponvalidation').style.color = 'green'
					document.getElementById('total').innerHTML = `${response.data.discountedtotal}&nbsp&nbsp$`
					document.getElementById('discount').innerHTML = `${response.data.discountamount}&nbsp&nbsp$`
					document.getElementById('couponvalidation').innerHTML = response.data.message
					setTimeout(() => { document.getElementById('couponvalidation').innerHTML =''},3000);
				}else{
					document.getElementById('couponvalidation').style.color = 'red'
					document.getElementById('total').innerHTML = `${response.data.discountedtotal}&nbsp&nbsp$`
					document.getElementById('discount').innerHTML = `${response.data.discountamount}&nbsp&nbsp$`
					document.getElementById('couponvalidation').innerHTML = response.data.message
					setTimeout(() => { document.getElementById('couponvalidation').innerHTML =''},3000);
				}
			})
		}
			
		
		
