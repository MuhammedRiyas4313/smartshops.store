		function countminus (idofitemminus) {

			let count = document.getElementById(`quantity${idofitemminus}`).value
			let price = document.getElementById(`productprice${idofitemminus}`).value
			let intcount = parseInt(count);
			let actprice = parseInt(price);
			if(intcount > 1){
			  
				axios.patch('/cart', {
				counter: intcount,
				itemDocId: idofitemminus,
				price: actprice,
				method:'decrement'
				})
				.then((data)=>{
					  if(data.data.user == 'noSession' ){
							Swal.fire({
							title: 'Oops !',
							text: "login please...",
							icon: 'question',
							showCancelButton: true,
							confirmButtonColor: '#3085d6',
							cancelButtonColor: '#d33',
							confirmButtonText: 'Yes, login !'
							}).then((result) => {
							if (result.isConfirmed) {
								location.href = '/user-login'
							}
							})
						}else if(data.data.message == 'decremented'){
						
						document.getElementById(`actualprice${idofitemminus}`).innerHTML = `${data.data.item.totalprice}&nbsp&nbsp$`
						document.getElementById('subtotal').innerHTML = `${data.data.grandtotal}&nbsp&nbsp$`
						document.getElementById('total').innerHTML = `${data.data.grandtotal}&nbsp&nbsp$`
						
					}else{
						Swal.fire(
                                  'Oops!',
                                  'Somethins gone wrong !',
                                  'error'
                                )   
					}
					
				}).catch((err)=>{
					console.log(err.message, 'from the cart error count minus')
					if(err){
						console.log(err.message, 'from the cart error count minus if err')
					}
				})
			  }	
			

		};
		
		function countplus (idofitemplus) {

				let count = document.getElementById(`quantity${idofitemplus}`).value
				let price = document.getElementById(`productprice${idofitemplus}`).value
				let actprice = parseInt(price);
				let intcount = parseInt(count);
					if(intcount > 0){
						axios.patch('/cart', {
						itemDocId: idofitemplus,
						price: actprice,
						method:'increment'
						})
						.then((data)=>{
							if(data.data.user == 'noSession' ){
								Swal.fire({
								title: 'Oops',
								text: "login please...",
								icon: 'question',
								showCancelButton: true,
								confirmButtonColor: '#3085d6',
								cancelButtonColor: '#d33',
								confirmButtonText: 'Yes, login !'
								}).then((result) => {
								if (result.isConfirmed) {
									location.href = '/user-login'
								 }
								})
							}else if(data.data.message == 'incremented'){
								document.getElementById(`actualprice${idofitemplus}`).innerHTML = `${data.data.item.totalprice}&nbsp&nbsp$`
								document.getElementById('subtotal').innerHTML = `${data.data.grandtotal}&nbsp&nbsp$`
								document.getElementById('total').innerHTML = `${data.data.grandtotal}&nbsp&nbsp$`
							}else{
								Swal.fire(
                                  'Oops!',
                                  data.data.message,
