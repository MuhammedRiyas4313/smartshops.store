		function applyCoupon(){
			let code = document.getElementById('couponcode').value
			let total = 
			console.log(code,'coupon code enetered')
			axios.post('/coupon',{
				code:code
			})
			.then((response) => {
				if(response.data.user == 'noSession' ){
					Swal.fire({
					title: 'Oops!',
					text: "login please...",
					icon: 'question',
					showCancelButton: true,
					confirmButtonColor: '#3085d6',
					cancelButtonColor: '#d33',
